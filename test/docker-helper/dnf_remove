#!/bin/bash

dnf  --nogpgcheck  -y  --rpmverbosity=error  --errorlevel=1  --color=auto remove $1
if [ $? == 0 ]; then
   exit 0
fi

dnf  --nogpgcheck  -y  --rpmverbosity=error  --errorlevel=1  --color=auto remove $1
if [ $? == 0 ]; then
   exit 0
fi

dnf  --nogpgcheck  -y  --rpmverbosity=error  --errorlevel=1  --color=auto remove $1

exit $?
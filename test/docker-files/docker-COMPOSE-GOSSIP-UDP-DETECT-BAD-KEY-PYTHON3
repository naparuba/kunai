version: '2'
services:
  node1:
    build:
       context: ../..
       dockerfile: test/docker-files/docker-file-DUO10-udp-detect-bad-key-node1.txt
       args:
          MY_PYTHON_VERSION: -python3
    networks:
       - gossip-udp-detect-bad-key-python3
    cap_add:
       - ALL

  node2:
    build:
       context: ../..
       dockerfile: test/docker-files/docker-file-DUO10-udp-detect-bad-key-node2.txt
       args:
          MY_PYTHON_VERSION: -python3
    networks:
       - gossip-udp-detect-bad-key-python3
    cap_add:
       - ALL




networks:
  gossip-udp-detect-bad-key-python3:
      driver: bridge

